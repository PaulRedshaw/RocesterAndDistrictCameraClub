<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>

<div class="panel panel-default">
    <div class="panel-heading">
        <h4>Where to find us?</h4>
    </div>
    <div id="map_canvas" style="width: 100%; height: 225px;" class=" panel-body" />
</div>
<script type="text/javascript">
    window.onload = initialize;
    function initialize() {
        var mapOptions = {
            center: new google.maps.LatLng(52.951769, -1.841562),
            zoom: 15,
            mapTypeId: google.maps.MapTypeId.HYBRID

        };
        var map = new google.maps.Map(document.getElementById("map_canvas"),
          mapOptions);
        // create a marker
        var latlng = new google.maps.LatLng(52.951769, -1.841562);
        var marker = new google.maps.Marker({
            position: latlng,
            map: map,
            title: 'Rocester Village Hall Committee Room',

        });
    }
</script>
